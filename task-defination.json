{
  "family": "product_recommendation_test",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "1024",
  "memory": "3072",
  "executionRoleArn": "arn:aws:iam::173732810069:role/ecsTaskExecutionRole",
  "taskRoleArn": "arn:aws:iam::173732810069:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "product-recommendation-api-test",
      "image": "ayushkore/product-recommendation-api",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 80,
          "protocol": "tcp"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/product_recommendation_test",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ]
}
